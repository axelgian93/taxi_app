{
  "name": "taxi_app_backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed",
    "bootstrap": "npx ts-node scripts/dev-bootstrap.ts",
    "reset-db": "npm run prisma:generate && npx prisma migrate reset --force && npm run bootstrap",
    "seed:test-trips": "npx ts-node scripts/seed-test-trips.ts",
    "clear:trips": "npx ts-node scripts/clear-trips.ts",
    "reseed:test-trips": "npm run clear:trips && npm run seed:test-trips",
    "smoke:e2e": "ts-node --transpile-only scripts/smoke-e2e.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.1.0",
    "@prisma/client": "^6.17.1",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.8.0",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
