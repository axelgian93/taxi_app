{
    "name":  "taxi_app_backend",
    "version":  "1.0.0",
    "private":  true,
    "type":  "commonjs",
    "scripts":  {
                    "dev":  "ts-node-dev --respawn --transpile-only src/index.ts",
                    "build":  "tsc -p tsconfig.build.json",
                    "start":  "node dist/index.js",
                    "lint": "eslint . --ext .ts --max-warnings=0",
                    "lint:fix": "eslint . --ext .ts --fix",
                    "bootstrap":  "npx ts-node scripts/dev-bootstrap.ts",
                    "seed:test-trips":  "npx ts-node scripts/seed-test-trips.ts",
                    "smoke:e2e":  "ts-node --transpile-only scripts/smoke-e2e.ts",
                    "smoke:push-token":  "ts-node --transpile-only scripts/smoke-push-token.ts",
                    "smoke:trip-sse":  "ts-node --transpile-only scripts/smoke-trip-sse.ts",
                    "prisma:generate":  "prisma generate",
                    "prisma:migrate":  "prisma migrate dev",
                    "prisma:deploy":  "prisma migrate deploy",
                    "smoke:stripe-webhook":  "ts-node --transpile-only scripts/smoke-stripe-webhook.ts",
                    "smoke:cancel-fee":  "ts-node --transpile-only scripts/smoke-cancel-fee.ts",
                    "openapi:export":  "ts-node --transpile-only scripts/export-openapi.ts",
                    "openapi:refresh-sdk":  "npm run openapi:export \u0026\u0026 docker run --rm -v %cd%/docs:/local openapitools/openapi-generator-cli generate -i /local/openapi.json -g dart-dio -o /local/dart-sdk \u0026\u0026 docker run --rm -v %cd%/docs:/local openapitools/openapi-generator-cli generate -i /local/openapi.json -g typescript-axios -o /local/ts-sdk",
                    "studio":  "prisma studio",
                    "smoke:refund-cash":  "ts-node --transpile-only scripts/smoke-refund-cash.ts",
                    "smoke:stripe-capture-refund":  "ts-node --transpile-only scripts/smoke-stripe-capture-refund.ts",
                    "smoke:stripe-cancel-auth":  "ts-node --transpile-only scripts/smoke-stripe-cancel-auth.ts"
                },
    "dependencies":  {
                         "@fastify/cors":  "^10.0.1",
                         "@fastify/helmet":  "^12.0.1",
                         "@fastify/jwt":  "^10.0.0",
                         "@fastify/rate-limit":  "^10.1.0",
                         "@fastify/swagger":  "^9.4.0",
                         "@fastify/swagger-ui":  "5.2.3",
                         "@prisma/client":  "^6.18.0",
                         "bcrypt":  "^5.1.1",
                         "dotenv":  "^16.4.5",
                         "fastify":  "^5.6.1",
                         "jsonwebtoken":  "^9.0.2",
                         "node-fetch":  "^3.3.2",
                         "pino-pretty":  "^11.2.2",
                         "prom-client":  "^15.1.3",
                         "stripe":  "^16.12.0"
                     },
    "devDependencies":  {
                            "@types/bcrypt":  "^5.0.2",
                            "@types/jsonwebtoken":  "^9.0.10",
                            "@types/node":  "^22.7.5",
                            "@typescript-eslint/eslint-plugin": "^8.10.0",
                            "@typescript-eslint/parser": "^8.10.0",
                            "eslint": "^9.14.0",
                            "eslint-config-prettier": "^9.1.0",
                            "eslint-plugin-import": "^2.29.1",
                            "prisma":  "^6.18.0",
                            "ts-node":  "^10.9.2",
                            "ts-node-dev":  "^2.0.0",
                            "typescript":  "^5.9.3"
                        }
}
